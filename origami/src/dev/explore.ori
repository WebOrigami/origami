(scope) => `<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width,initial-scale=1" />
    <title>Origami Explorer</title>
    <style>${ <explore.css> }</style>
    <script>${ <explore.inline.js> }</script>
  </head>
  <body>
    <nav>
    <div id="label">Origami Explorer</div>
    <div id="scopeToolbar">
      <button id="buttonContent">Content</button>
      <button id="buttonIndex">Index</button>
      <button id="buttonSVG">SVG</button>
      <button id="buttonYAML">YAML</button>
    </div>
    ${ Tree.map(scope, (scopeTree, index) => `
      <ul>
        <h2>${ scopeTree.name ?? "" }</h2>
        ${ Tree.map(scopeTree.keys, (key) => `
        <li>
          <a href="./!explore/${ index > 0 ? "!" : "" }${ key }" target="frame">${ key }</a>
        </li>
        `) }
      </ul>
    `) }
    </nav>
    <iframe id="frame" name="frame"></iframe>
  </body>
</html>
`
