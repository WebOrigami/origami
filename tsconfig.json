{
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "allowSyntheticDefaultImports": true,
    "checkJs": true,
    "lib": ["ES2022", "DOM"],
    "noEmit": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strict": true,
    "target": "ES2022"
  },
  "include": ["**/*.js"],
  "exclude": ["origami/exports/exports.js", "origami/src/dev/explore.inline.js"]
}
